      "source": [
        "# 台形の各辺の長さ\n",
        "upper_base = 10  # 上辺\n",
        "lower_base = 20  # 下辺\n",
        "height = 5       # 高さ\n",
        "\n",
        "# 台形の面積を計算\n",
        "area = (upper_base + lower_base) * height / 2\n",
        "\n",
        "# 結果を出力\n",
        "print(f\"{area}cm²\")"
      ]
    }
  ]
